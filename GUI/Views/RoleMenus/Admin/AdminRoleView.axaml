<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:GUI.ViewModels.RoleMenus.Admin"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="GUI.Views.RoleMenus.Admin.AdminRoleView"
        x:DataType="vm:AdminRoleViewModel">

<StackPanel HorizontalAlignment="Center" Orientation="Vertical" Width="1000" Margin="20">
        <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="_Главное Меню (Деканат)" Command="{Binding OpenMenuCommand}" />
            <MenuItem Header="_Терминал" Command="{Binding OpenTerminalCommand}"/>
            <MenuItem Header="_Справочники">
                <MenuItem Header="Города" Command="{Binding OpenCityCommand}" />
                <MenuItem Header="Улицы" Command="{Binding OpenStreetCommand}" />
            </MenuItem>
            <MenuItem Header="_Пользователи" Command="{Binding OpenUsersCommand}" />
            <MenuItem Header="_Аудит" Command="{Binding OpenAuditCommand}" />
        </Menu>
        </DockPanel>
</StackPanel>

</UserControl>
